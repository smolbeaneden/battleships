{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "./env.d.ts",
    "UI/src/**/*",
    "UI/src/**/*.vue"
  ],
  "exclude": [
    "UI/src/**/__tests__/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "allowImportingTsExtensions": true,

    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
